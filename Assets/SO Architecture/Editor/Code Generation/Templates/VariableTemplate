using UnityEngine;

namespace ScriptableObjectArchitecture
{
	[CreateAssetMenu(
	    fileName = "$TYPE_NAME$Variable.asset",
	    menuName = SOArchitecture_Utility.VARIABLE_SUBMENU + "$MENU_NAME$",
	    order = $ORDER$)]
	public class $TYPE_NAME$Variable : BaseVariable<$TYPE$>
	{
		public static $TYPE_NAME$Variable CreateAsset() => EditorAssistantUtility.CreateAsset<$TYPE_NAME$Variable>();
	}
}